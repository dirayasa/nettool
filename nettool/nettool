			nettool
		else
			clear
			echo "Perintah salah"
			sleep 3
			exit
		fi
	elif [ $opsi -eq 5 ]; then
		if [ -f /system/etc/init.d/nanocop >/dev/null ]; then
			echo "Android sudah di optimasi, enter untuk kembali"
			read enterKey
			nettool
		else
			echo -n "Cek koneksi"
			echo -n "."
			sleep 1
			echo -n "."
			sleep 1
			echo -n "."
			sleep 1
			if ping -q -c 1 -W 1 google.com >/dev/null; then
				echo "\e[1;32m OK!\e[0m"
				sleep 1
				echo "Mengambil berkas dari server..."
				wget -O $tmpsd/nanocop.txt http://sudonano.pe.hu/optimusnano/nanocop &> /dev/null
				wget -O $tmpsd/sysctl.conf http://sudonano.pe.hu/optimusnano/sysctl.conf &> /dev/null
				sleep 2
				echo $pasangsistem
				mv $tmpsd/nanocop.txt /system/etc/init.d/nanocop &> /dev/null
				mv $tmpsd/sysctl.conf /system/etc/sysctl.conf &> /dev/null
				sleep 2
				echo $ijinsistem
				chmod 777 /system/etc/init.d/nanocop &> /dev/null
				chmod 644 /system/etc/sysctl.conf &> /dev/null
				sleep 2
				echo "Android sudah teroptimasi, reboot untuk menerapkan optimasi"
				echo $balek
				read enterKey
				nettool
			else
				echo "\e[1;31m GAGAL!\e[0m"
				sleep 1
				echo ""
				echo "Koneksi mati atau tidak bisa di gunakan, silahkan cek koneksi dan ulangi kembali"
				echo $balek
				read enterKey
				nettool
			fi
		fi
