			sleep 1
			echo "Membuat file swap..."
			mkswap /dev/block/zram0
			sleep 1
			echo "Menghidupkan Swap"
			swapon /dev/block/zram0
			echo "Swap sudah dirubah menjadi 0,5 GB, $balek"
			read enterKey
			nettool
