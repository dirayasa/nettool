			sleep 1
			echo "Membuat file swap..."
			mkswap /dev/block/zram0
			sleep 1
			echo "Menghidupkan Swap"
			swapon /dev/block/zram0
			echo "Swap sudah dirubah menjadi 1,5 GB, $balek"
			read enterKey
			nettool
		elif [ $swapchoose -eq 4 ]; then
			echo "Swap di ubah menjadi 2 GB.."
			sleep 1
