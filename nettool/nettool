			echo 1024M > /sys/block/zram0/disksize
			sleep 1
			echo "Membuat file swap..."
			mkswap /dev/block/zram0
			sleep 1
			echo "Menghidupkan Swap"
			swapon /dev/block/zram0
			echo "Swap sudah dirubah menjadi 1 GB, $balek"
			read enterKey
			nettool
		elif [ $swapchoose -eq 3 ]; then
			echo "Swap di ubah menjadi 1,5 GB.."
