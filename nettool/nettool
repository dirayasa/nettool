		if [ `du -shc $sampahmelimpah 2> /dev/null | tail -1 | cut -f 1` == 0 ]; then
			echo -n "\e[1;32mPerangkat kamu sudah dibersihkan...\e[0m"
			echo \e[1;32m$balek\e[0m
			read enterKey
			nettool
		else
			echo -n "Cache Aplikasi: \e[1;31m"
			echo `du -shc $sampahmelimpah 2> /dev/null | tail -1 | cut -f 1`
			echo "\e[0m"
			echo -n "Ingin membersihkannya (y/n)? "
			read jawab
			if [ $jawab == y ]; then
				echo -n "Sedang menghapus sampah"
				sleep 1
				echo -n "."
				echo `du -shc $sampahmelimpah 2> /dev/null | tail -1 | cut -f 1` >> $tmpsd/total.txt
				rm -f $sampahmelimpah 2> /dev/null
				rm -rf $sampahmelimpah 2> /dev/null
				sleep 1
				echo -n "."
				rm -f /sdcard/DCIM/.thumbnails/* 2> /dev/null
				if [ -e /dev/log/main ]; then
				  rm -f /dev/log/main
				fi
				rm -f /data/dalvik-cache/*.apk
				rm -f /data/dalvik-cache/*.tmp
				sleep 1
				echo "."
				bebas=`cat $tmpsd/total.txt`
				echo ""
				echo "\e[1;32m$bebas sudah dibebaskan!\e[0m"
				echo $balek
				read enterKey
				rm -f $tmpsd/total.txt
				nettool
			elif [ $jawab == n ]; then
				nettool
			else
				echo "Perintah Salah"
				sleep 3
				exit
			fi
		fi
		done
	elif [ $opsi -eq 2 ]; then
		echo ""
		echo -n "Menghitung APK..."
		echo ""
		if [ `find /sdcard/ -type f -name "*.apk" 2> /dev/null | wc -l` == 0 ]; then
			echo -n "\e[1;32mFile APK lama sudah dibebaskan semua, enter untuk kembali\e[0m"
			read enterKey
			nettool
		else
			calc=`find /sdcard/ -type f -name "*.apk" 2> /dev/null | wc -l`
			echo -n "Total Sampah APK $calc file, ingin menghapusnya (y/n)? "
			read jawab
			if [ $jawab == y ]; then
				echo -n "Sedang menghapus file APK"
				sleep 1
				echo -n "."
				echo `find /sdcard/ -type f -name "*.apk" 2> /dev/null | wc -l` >> $tmpsd/total.txt
				sleep 1
				echo -n "."
				sleep 1
				echo -n "."
				sleep 1
				echo -n "."
				sleep 1
				echo -n "."
				rm -f $fullapk 2> /dev/null
				echo ""
				totalapknya=`cat $tmpsd/total.txt`
				echo "\e[1;32m$totalapknya APK berhasil dihapus!\e[0m"
				echo $balek
				read enterKey
				rm -f $tmpsd/total.txt
				nettool
			elif [ $jawab == n ]; then
				nettool
			else
				echo "Perintah Salah"
				sleep 3
				exit
			fi
		fi
	elif [ $opsi -eq 3 ]; then
		echo ""
		echo "Menghitung Dalvik Cache..."
		sleep 3
		echo -n "Total Dalvik Cache: "
		echo `du -shc /data/dalvik-cache/* 2> /dev/null | tail -1 | cut -f 1`
		echo ""
		sleep 1
		echo "Menghapus akan membuat booting awal menunggu lama dan membuat perangkat fresh"
		echo "Proses akan reboot otomatis"
		echo -n "Ingin menghapus Dalvik Cache (y/n)? "
		read jawab
		if [ $jawab == y ]; then
			echo -n "Sedang menghapus Dalvik Cache"
			sleep 1
			echo -n "."
			sleep 1
			echo -n "."
			sleep 1
			echo -n "."
			sleep 1
			echo -n "."
			sleep 1
			echo -n "."
			echo `du -shc /data/dalvik-cache/* 2> /dev/null | tail -1 | cut -f 1` >> $filetmp/totaldc.txt
			rm -rf /data/dalvik-cache/* 2> /dev/null
			bebas=`cat $filetmp/totaldc.txt`
			echo ""
			echo "\e[1;32m$bebas sudah dibebaskan dari Dalvik Cache!\e[0m"
			echo -n "Perangkat akan di nyalakan ulang dalam 5 detik"
			sleep 1
			echo -n "."
			sleep 1
			echo -n "."
			sleep 1
			echo -n "."
			sleep 1
			echo -n "."
			sleep 1
			echo "."
			rm -rf $filetmp/totaldc.txt
			reboot
		elif [ $jawab == n ]; then
			nettool
		else
			echo "Perintah Salah"
			sleep 3
			exit
		fi
	elif [ $opsi -eq 4 ]; then
		clear
		echo "=========================================================="
		echo "                      Kocok Nomer                         "
		echo "=========================================================="
		echo ""
		echo -n "No. Maksimal: "
		read nomer
		if [ $nomer -gt 0 ]; then
			sleep 1
