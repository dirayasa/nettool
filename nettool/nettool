		rm -f /system/bin/airdecloak-ng &> /dev/null
		rm -f /system/bin/aireplay-ng &> /dev/null
		rm -f /system/bin/airodump-ng &> /dev/null
		rm -f /system/bin/airolib-ng &> /dev/null
		rm -f /system/bin/airserv-ng &> /dev/null
		rm -f /system/bin/airtun-ng &> /dev/null
		rm -f /system/bin/besside-ng &> /dev/null
		rm -f /system/bin/besside-ng-crawler &> /dev/null
		rm -f /system/bin/buddy-ng &> /dev/null
		rm -f /system/bin/easside-ng &> /dev/null
		rm -f /system/bin/ifrename &> /dev/null
		rm -f /system/bin/ivstools &> /dev/null
		rm -f /system/bin/iwconfig &> /dev/null
		rm -f /system/bin/iwevent &> /dev/null
		rm -f /system/bin/iwgetid &> /dev/null
		rm -f /system/bin/iwlist &> /dev/null
		rm -f /system/bin/iwmulticall &> /dev/null
		rm -f /system/bin/iwpriv &> /dev/null
		rm -f /system/bin/iwspy &> /dev/null
		rm -f /system/bin/kstats &> /dev/null
		rm -f /system/bin/macaddr &> /dev/null
		rm -f /system/bin/makeivs-ng &> /dev/null
		rm -f /system/bin/packetforge-ng &> /dev/null
		rm -f /system/bin/tcpdump &> /dev/null
		rm -f /system/bin/tkiptun-ng &> /dev/null
		rm -f /system/bin/wesside-ng &> /dev/null
		rm -f /system/bin/wmon &> /dev/null
		rm -f /system/bin/wpaclean &> /dev/null
		rm -f /system/xbin/airbase-ng &> /dev/null
		rm -f /system/xbin/aircrack-ng &> /dev/null
		rm -f /system/xbin/airdecap-ng &> /dev/null
		rm -f /system/xbin/airdecloak-ng &> /dev/null
		rm -f /system/xbin/aireplay-ng &> /dev/null
		rm -f /system/xbin/airodump-ng &> /dev/null
		rm -f /system/xbin/airolib-ng &> /dev/null
		rm -f /system/xbin/airserv-ng &> /dev/null
		rm -f /system/xbin/airtun-ng &> /dev/null
		rm -f /system/xbin/besside-ng &> /dev/null
		rm -f /system/xbin/besside-ng-crawler &> /dev/null
		rm -f /system/xbin/buddy-ng &> /dev/null
		rm -f /system/xbin/easside-ng &> /dev/null
		rm -f /system/xbin/ifrename &> /dev/null
		rm -f /system/xbin/ivstools &> /dev/null
		rm -f /system/xbin/iwconfig &> /dev/null
		rm -f /system/xbin/iwevent &> /dev/null
		rm -f /system/xbin/iwgetid &> /dev/null
		rm -f /system/xbin/iwlist &> /dev/null
		rm -f /system/xbin/iwmulticall &> /dev/null
		rm -f /system/xbin/iwpriv &> /dev/null
		rm -f /system/xbin/iwspy &> /dev/null
		rm -f /system/xbin/kstats &> /dev/null
		rm -f /system/xbin/macaddr &> /dev/null
		rm -f /system/xbin/makeivs-ng &> /dev/null
		rm -f /system/xbin/packetforge-ng &> /dev/null
		rm -f /system/xbin/tcpdump &> /dev/null
		rm -f /system/xbin/tkiptun-ng &> /dev/null
		rm -f /system/xbin/wesside-ng &> /dev/null
		rm -f /system/xbin/wmon &> /dev/null
		rm -f /system/xbin/wpaclean &> /dev/null
		if [ -f $filetmp/tosnettool ]; then rm -f $filetmp/tosnettool 2> /dev/null; fi
		if [ -f $filetmp/tosnettool ]; then rm -f $filetmp/tosnettool 2> /dev/null; fi
		if [ -f $tmpsd/ShowIp.txt ]; then rm -f $tmpsd/ShowIp.txt 2> /dev/null; fi
		if [ -f $tmpsd/backupalamathwmanual ]; then rm -f $tmpsd/backupalamathwmanual 2> /dev/null; fi
		if [ -f $tmpsd/backupalamathw ]; then rm -f $tmpsd/backupalamathw 2> /dev/null; fi
		if [ -f $filetmp/.bootkali.sh ]; then rm -f $filetmp/.bootkali.sh 2> /dev/null; fi
		if [ -f $filetmp/hostterinstall ]; then rm -f $filetmp/hostterinstall 2> /dev/null; fi
		if [ -f /system/etc/hosts ]; then rm -f /system/etc/hosts 2> /dev/null; fi
		echo '127.0.0.1 localhost' >> /system/etc/hosts
		echo '::1 localhost #[IPv6]' >> /system/etc/hosts
		chmod 644 /system/etc/hosts
		if [ -d /system/bin/NMAP_for_android/ ]; then rm -rf /system/bin/NMAP_for_android/ 2> /dev/null; fi
		if [ -f $filetmp/.bootkali.sh ]; then rm -f $filetmp/.bootkali.sh 2> /dev/null; fi
		if [ -f $filetmp/.runkali.sh ]; then rm-f  $filetmp/.runkali.sh 2> /dev/null; fi
		if [ -d /data/bin/ ]; then rm -rf /data/bin/ 2> /dev/null; fi
		if [ -f /system/bin/nmap* ]; then rm -f /system/bin/nmap* 2> /dev/null; fi
		if [ -f /system/bin/foo ]; then rm -f /system/bin/foo 2> /dev/null; fi
		if [ -f $tmpsd/nmap ]; then rm -f $tmpsd/nmap 2> /dev/null; fi
		if [ -d /data/local/mnt/ ]; then rm -rf /data/local/mnt/ 2> /dev/null; fi
		if [ -d $tmpsd/Whois/ ]; then rm -rf $tmpsd/Whois/ 2> /dev/null; fi
		if [ -f /system/bin/nettool ]; then rm -f /system/bin/nettool 2> /dev/null; fi
		exit
	elif [ $opsi -eq 101 ]; then
		clear
		echo -n "Mengecek Pembaruan"
			sleep 1
			echo -n "."
			sleep 1
			echo -n "."
			sleep 1
			echo "."
				if [ `curl -sI http://sudonano.pe.hu/nettool/nettool | grep Length: | awk '{print $2}'` == `wc -c /system/bin/nettool | awk '{print $1}'` &> /dev/null ]; then
					sleep 1
					echo "NETTOOL sudah terbaru, enter untuk kembali..."
					read enterKey
					nettool
				else
					sleep 1
					echo "NETTOOL akan di perbarui..."
					sleep 1
					echo "Mengunduh pembaruan..."
					wget -O $tmpsd/netme http://sudonano.pe.hu/nettool/nettool &> /dev/null
					echo -n "Memasang berkas kedalam sistem"
					sleep 1
					echo -n "."
					sleep 1
					mv $tmpsd/netme /system/bin/nettool &> /dev/null
					echo -n "."
					chmod 755 /system/bin/nettool &> /dev/null
					sleep 1
					echo "."
					echo "NETTOOL telah diperbarui, memulai ulang NETTOOL"
					sleep 3
					nettool
				fi
	elif [ $opsi -eq 102 ]; then
		echo ""
		echo "Ingin melakukan memulai ulang? Y/n"
		read matiin
		for oke in y Y ya Yes YEs YES yes Ya YA yA yEs YeS yES
		do
		if [ $matiin == $oke ]; then
			reboot
		else
			nettool
		fi
		done
	elif [ $opsi -eq 110 ]; then
		clear
		echo "Menerapkan Optimasi Baterai"
		echo ""
		sh /system/etc/init.d/nanocop
		echo ""
		echo $balek
		read enterKey
		nettool
	elif [ $opsi -eq 111 ]; then
		nettool
	else
		clear
		echo "Perintah Salah"
		sleep 3
		exit
	fi
elif [ $option -eq 21 ]; then
	for dataseeder in /system/bin/seeder /system/xbin/rngd /system/xbin/entro
		do
		if [[ -f $dataseeder ]]; then
			clear
			seeder
		else
			echo "Binary tidak dapat ditemukan"
			echo -n "Mengambil data Entropy Generator dari server"
			wget -O /data/lognettool/seederbin http://sudonano.pe.hu/seeder/bin/seeder &> /dev/null
			sleep 1
			echo -n "."
			wget -O /data/lognettool/rngdxbin http://sudonano.pe.hu/seeder/xbin/rngd &> /dev/null
			sleep 1
			echo -n "."
			wget -O /data/lognettool/entroxbin http://sudonano.pe.hu/seeder/xbin/entro &> /dev/null
			sleep 1
			echo "."
			sleep 1
			echo "Memasang ke sistem..."
			mv /data/lognettool/seederbin /system/bin/seeder
			mv /data/lognettool/rngdxbin /system/xbin/rngd
			mv /data/lognettool/entroxbin /system/xbin/entro
			echo "Memberikan perijinan sistem..."
			chmod 755 /system/bin/seeder
			chmod 755 /system/xbin/rngd
			chmod 755 /system/xbin/entro
			sleep 1
			echo "Mengecek file..."
			sleep 1
			for data in /system/bin/seeder /system/xbin/rngd /system/xbin/entro
			do
				if [ -f $data ]; then
					echo "Data Lengkap!"
					sleep 2
					seeder
				else
					echo "Data terdapat yang kurang, ulangi kembali..."
					sleep 2
					nettool
				fi
			done
		fi
	done
elif [ $option -eq 0 ]; then
	nettool
else
	clear
	echo "Perintah salah"
	sleep 2
	exit
fi